{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode dev",
    "tez-dev": "tez dev --mode dev",
    "pre.live": "vite --mode pre.live",
    "live": "vite --mode live",
    "stage": "node --max-old-space-size=10240 node_modules/vue-tsc/bin/vue-tsc --noEmit && vite build --mode stage",
    "build": "node --max-old-space-size=8192 node_modules/vue-tsc/bin/vue-tsc --noEmit && vite build --mode pre.live",
    "generate:pre:live": "node --max-old-space-size=4096 node_modules/vue-tsc/bin/vue-tsc --noEmit && vite build --mode pre.live",
    "generate:stage": "node --max-old-space-size=4096 node_modules/vue-tsc/bin/vue-tsc --noEmit && vite build --mode stage",
    "serve": "vite preview",
    "preview": "vite preview",
    "serve-functions": "set NODE_ENV=development && firebase emulators:start --only functions --token 1//0gKTjJ2JBASF6CgYIARAAGBASNwF-L9IrLppOiH_pJwm1pPRbMULCYScNpe1TrjBGNI5flYnH649Sg1hdkNRg13JTPj-9bF6ncnE",
    "develop:backend": "npm run serve-functions",
    "develop:frontend": "wait-on  http://localhost:4000/functions && npm run dev",
    "develop": "cross-env FORCE_COLOR=1 npm-run-all -l -p develop:*",
    "deploy:functions:stage": "npm run set:env:stage && firebase deploy --only functions --token 1//0gKTjJ2JBASF6CgYIARAAGBASNwF-L9IrLppOiH_pJwm1pPRbMULCYScNpe1TrjBGNI5flYnH649Sg1hdkNRg13JTPj-9bF6ncnE --project radixweb-stage",
    "deploy:functions:prelive": "npm run set:env:prelive && firebase deploy --only functions --token 1//0gKTjJ2JBASF6CgYIARAAGBASNwF-L9IrLppOiH_pJwm1pPRbMULCYScNpe1TrjBGNI5flYnH649Sg1hdkNRg13JTPj-9bF6ncnE --project radixweb-prelive",
    "set:env:stage": "firebase functions:config:set runtime.env=stage --token 1//0gKTjJ2JBASF6CgYIARAAGBASNwF-L9IrLppOiH_pJwm1pPRbMULCYScNpe1TrjBGNI5flYnH649Sg1hdkNRg13JTPj-9bF6ncnE --project radixweb-stage",
    "set:env:prelive": "firebase functions:config:set runtime.env=prelive --token 1//0gKTjJ2JBASF6CgYIARAAGBASNwF-L9IrLppOiH_pJwm1pPRbMULCYScNpe1TrjBGNI5flYnH649Sg1hdkNRg13JTPj-9bF6ncnE --project radixweb-prelive"
  },
